# Kconfig settings for AR100

if MACH_AR100

config AR100_SELECT
    bool
    default y
    select HAVE_ENDSTOPS
    select HAVE_STEPPERS
    select HAVE_GPIO
    select HAVE_STEPPER_BOTH_EDGE

config STEP_DELAY
    int
    default 0

config BOARD_DIRECTORY
    string
    default "ar100"

config CLOCK_FREQ
    int
    default 300000000

endif
